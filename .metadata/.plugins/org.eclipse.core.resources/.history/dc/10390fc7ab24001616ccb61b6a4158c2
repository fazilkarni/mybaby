package com.intuit.java8.functional.interfaces;

public class FunctionInterfaceDemo {

	public static void main(String[] args) { 
		// TODO Auto-generated method stub
		FunctionalInterface1<String,Integer> funIntr = Integer::valueOf;
		System.out.println(funIntr.add(10,10));
		System.out.println(funIntr.convert("10"));
		int a = 10;
		FunctionalInterface1<String,Integer> funIntr2 = (from)-> (a+Integer.valueOf(from));
		System.out.println(funIntr2.convert("10"));
	}

}

@FunctionalInterface
interface FunctionalInterface1<F,T> {
	default int add(int a, int b){
		return a+b;
	}
	default int multiply(int a, int b){
		return a*b;
	}
	
	int convert(String a);
	
}
